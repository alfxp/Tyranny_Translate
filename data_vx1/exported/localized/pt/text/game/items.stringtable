      <ID>1937</ID>
      <DefaultText>Crista da onda</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1938</ID>
      <DefaultText>Chorando sussurros</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1939</ID>
      <DefaultText>Razão Circular</DefaultText>
      <FemaleText />
