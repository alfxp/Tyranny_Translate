      <ID>4065</ID>
      <DefaultText>Recruit Hired</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4066</ID>
      <DefaultText>{0} will appear the next time you travel to the {1}.</DefaultText>
      <FemaleText />
