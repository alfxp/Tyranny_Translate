      <ID>359</ID>
      <DefaultText>Sun Soldier's Javelin</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>360</ID>
      <DefaultText>Sun Soldier's Shield</DefaultText>
      <FemaleText />
    </Entry>
