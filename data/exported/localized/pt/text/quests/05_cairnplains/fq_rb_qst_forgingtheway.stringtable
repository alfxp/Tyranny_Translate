    <Entry>
      <ID>4</ID>
      <DefaultText>Lidar com os invasores no Jagged Maw Shrine.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText>Derrote cem-sangue.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>6</ID>
