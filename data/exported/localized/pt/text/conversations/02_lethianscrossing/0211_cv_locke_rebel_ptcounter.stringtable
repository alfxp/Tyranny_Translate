    <Entry>
      <ID>45</ID>
      <DefaultText>[Leave] "I will speak to Myrek."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>46</ID>
      <DefaultText>"Eu não tenho idéia, Myrek provavelmente faz, no entanto."</DefaultText>
