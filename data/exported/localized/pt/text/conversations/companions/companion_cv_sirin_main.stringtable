    <Entry>
      <ID>362</ID>
      <DefaultText>"Ela não tentou salvar? Isso é muito insensível."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>363</ID>
      <DefaultText>"Se se voltaram para ser tão rápido? Por que uma repentina?"</DefaultText>
      <FemaleText />
